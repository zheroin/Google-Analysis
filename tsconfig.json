{
  "compilerOptions": {
    "outDir": "./public/",
    "sourceMap": true,
    "strict": true,
    "module": "esnext",
    "moduleResolution": "node",
    "jsx": "react",
    "target": "es5",
    "allowSyntheticDefaultImports": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "downlevelIteration": true,
    "noImplicitAny": false,
    "lib": ["dom.iterable", "dom", "esnext"],
    "types": ["gapi", "gapi.auth2", "gapi.client", "gapi.client.analytics"]
  },
  "include": [
    "./src/"
  ]
}
